{
    "extends": "standard",
    "globals": {
        "fetch": true,
        "moment": true,
        "$": true
    },
    "rules": {
        "indent": 0,
        "no-tabs": 0,
        "no-unused-vars": 0,
        "padded-blocks": 0,
        "handle-callback-err": 0,
        "semi": 0,
        "space-before-function-paren": 0,
        "space-before-blocks": 0,
        "no-trailing-spaces": 0,
        "no-mixed-spaces-and-tabs": 0
    }
}
